<style>
  .svgpeople.animated {
    animation-delay: 1s;
    stroke:#000;
    fill:none;
    stroke-dasharray: 1000px;
    stroke-dashoffset: 1000px;
    animation-name: dash;
    animation-duration: 5.0s;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
    animation-timing-function: ease;
  }

  .svgprocess.animated {
    animation-delay: 3s;
    stroke:#000;
    fill:none;
    stroke-dasharray: 1000px;
    stroke-dashoffset: 1000px;
    animation-name: dash;
    animation-duration: 5.0s;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
    animation-timing-function: ease;
  }

  .svgproduct.animated {
    animation-delay: 5s;
    stroke:#000;
    fill:none;
    stroke-dasharray: 1000px;
    stroke-dashoffset: 1000px;
    animation-name: dash;
    animation-duration: 5.0s;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
    animation-timing-function: ease;
  }

  @keyframes dash {
  to {
    stroke-dashoffset: 0;
    }
  }
</style>

<script is:inline>
  document.querySelectorAll('.hibbem').forEach(function(target) {
    target.style.display = 'none';
  });

  document.getElementById("windmill").pauseAnimations();
  document.getElementById("windmill-container").onclick = function(){
    document.getElementById("windmill").unpauseAnimations();
    document.querySelectorAll('.hibbem').forEach(function(target) {
      target.style.display = 'block';
    });
  };

  function onVisible(element, callback) {
    new IntersectionObserver((entries, observer) => {
      entries.forEach(entry => {
        if(entry.intersectionRatio > 0) {
          callback(element);
          observer.disconnect();
        }
      });
    }).observe(element);
    if(!callback) return new Promise(r => callback=r);
  }

  onVisible(document.querySelector("#windmill"), () => 
    document.querySelectorAll('.svgpeople, .svgprocess, .svgproduct').forEach(function(target) {
      target.classList.add("animated");
    })  
  ); 
</script>